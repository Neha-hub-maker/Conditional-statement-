#include<stdio.h>
#include<math.h>
int main(){

float a,b,c,d,x1,x2;
printf("enter numbers of your hsc:\n");
scanf("%f,%f,%f",&a,&b,&c);
d= b*b-4*a*c;
if(d==0)

{
x1= (-b+ sqrt(b*b-4*a*c)/2*a);
x1=x2;
 printf("both roots are equal");   

}
else if(d>0)
    {
x1 = (-b + sqrt(d)) / (2 * a);   // Calculate the first root.
       x2 = (-b - sqrt(d)) / (2 * a);  

        printf("roots are %f anf %f",&x1,&x2);

    }
    else{

printf("roots are imaginary");
    }
  



    return 0;
}
